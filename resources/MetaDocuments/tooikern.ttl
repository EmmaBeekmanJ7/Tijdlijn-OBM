  skos:inScheme tooikern:soortKennisgeving ;
  skos:prefLabel "kennisgeving participatie"@nl ;
  skos:scopeNote "Een kennisgeving participatie kan gecombineerd worden met de kennisgeving voorgenomen besluit."@nl ;
